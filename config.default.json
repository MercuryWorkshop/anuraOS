{
	"apps": [
		"apps/term.app",
		"apps/fsapp.app",
		"apps/marketplace.app",
		"apps/ashell.app"
	],
	"libs": [
		"/apps/libfileview.lib",
		"/apps/libfilepicker.lib",
		"/apps/libstore.lib",
		"/apps/libpersist.lib",
		"/apps/libhterm.lib"
	],
	"bin": ["/bin/chimerix.ajs"],
	"defaultsettings": {
		"use-sw-cache": false,
		"applist": ["anura.browser", "anura.settings", "anura.fsapp"],
		"clampWindows": true,
		"launcher-keybind": true,
		"relay-url": "wss://relay.widgetry.org/"
